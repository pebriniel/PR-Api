{% extends 'base.html.twig' %}

{% block body %}
    <h1>Appskill</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ appSkill.id }}</td>
            </tr>
            <tr>
                <th>Category</th>
                <td>{{ appSkill.category }}</td>
            </tr>
            <tr>
                <th>Order</th>
                <td>{{ appSkill.order }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('appskills_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('appskills_edit', { 'id': appSkill.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
