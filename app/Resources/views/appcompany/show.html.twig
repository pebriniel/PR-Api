{% extends 'base.html.twig' %}

{% block body %}
    <h1>Appcompany</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ appCompany.id }}</td>
            </tr>
            <tr>
                <th>Company</th>
                <td>{{ appCompany.company }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ appCompany.url }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ appCompany.logo }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('appcompany_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('appcompany_edit', { 'id': appCompany.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
